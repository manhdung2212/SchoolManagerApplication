@model SchoolManager.Models.Subject

<h2>Detail</h2>
<table class="table table-hover">
    <tr>
        <th>ID : </th>
        <td>@Model.ID</td>
    </tr>

    <tr>
        <th>Name : </th>
        <td>@Model.Name</td>
    </tr>

    <tr>
        <th>node : </th>
        <td>@Model.Node</td>
    </tr>

    <tr>
        <th>createdate : </th>
        <td>@Model.CreateDate</td>
    </tr>

    <tr>
        <th>updatedate : </th>
        <td>@Model.UpdateDate</td>
    </tr>

    <tr>
        <th>createby : </th>
        <td>@Model.CreateBy</td>
    </tr>

    <tr>
        <th>updateby : </th>
        <td>@Model.UpdateBy</td>
